{
  "presets": ["env", "react"],
	"plugins": [
		["transform-react-jsx", {
			"pragma": "h"
		}]
	],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-inline-elements",
        "transform-react-constant-elements"
      ]
    }
  }
}
